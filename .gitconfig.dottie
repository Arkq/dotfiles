[user]
	name = Arkadiusz Bokowy
	email = arkadiusz.bokowy@gmail.com
[alias]
	ad = add
	cm = commit
	co = checkout
	df = diff
	dt = difftool
	pl = pull
	pr = pull --rebase=preserve
	s = status --short --branch
	sh = show
	st = status
	dissect = bisect
[log]
	decorate = auto
[push]
	default = simple
[rebase]
	autosquash = true
	autostash = true
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
